{
  "name": "my-store",
  "version": "1.0.0",
  "description": "Store online with node",
  "main": "server.js",
  "author": "Joseph Boscan",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon server.js",
    "start": "node server.js",
    "migrations:generate": "sequelize-cli migration:generate --name",
    "migrations:run": "npx sequelize-cli db:migrate",
    "migrations:revert": "npx sequelize-cli db:migrate:undo",
    "migrations:delete": "npx sequelize-cli db:migrate:undo:all",
    "lint": "eslint"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.5.0",
    "dotenv": "^16.0.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.3.2",
    "sequelize-cli": "^6.4.1"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "joi": "^17.6.0",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.23.1"
  }
}
